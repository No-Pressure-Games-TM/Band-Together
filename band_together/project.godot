; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Band Together"
run/main_scene="res://scenes/interfaces/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://assets/icon.svg"

[autoload]

SceneTransition="*res://scenes/scene_transition/scene_transition.tscn"
UI="*res://scenes/ui.tscn"
GameManager="*res://scenes/game_manager.gd"
AudioManager="*res://scenes/audio_manager.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"allegro": "res://allegro.dch"
}
directories/dtl_directory={
"door1": "res://dialogic timelines/door1.dtl",
"findbaton": "res://dialogic timelines/findbaton.dtl",
"finddrum": "res://dialogic timelines/finddrum.dtl",
"findsax": "res://dialogic timelines/findsax.dtl",
"findviolin": "res://dialogic timelines/findviolin.dtl",
"prologue": "res://dialogic timelines/prologue.dtl"
}
glossary/default_case_sensitive=true
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
layout/style_list=["res://dialogic timelines/dialogic_style.tres"]
layout/default_style="res://dialogic timelines/dialogic_style.tres"
variables={}
audio/channel_defaults={
"": {
"audio_bus": "",
"fade_length": 0.0,
"loop": false,
"volume": 0.0
},
"music": {
"audio_bus": "",
"fade_length": 0.0,
"loop": true,
"volume": 0.0
}
}

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1920
window/size/window_height_override=1080
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="Band Together"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

[file_customization]

folder_colors={
"res://Environment/": "teal",
"res://assets/": "pink",
"res://audio/": "orange",
"res://entities/": "yellow",
"res://scenes/": "blue"
}

[global_group]

enemy="Enemies that can be hit with attacks!"

[gui]

theme/custom_font="res://assets/GrapeSoda.ttf"

